{
  "name": "react-component-click-handler",
  "version": "0.9.0",
  "description": "pass through component for when you need just a click handler to pass back some data",
  "main": "index.js",
  "author": "Kelly Milligan",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/kellyrmilligan/react-component-click-handler.git"
  },
  "jest": {
    "roots": [
      "src"
    ],
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "moduleFileExtensions": [
      "js",
      "json"
    ],
    "coverageDirectory": "coverage",
    "collectCoverage": true,
    "testRegex": "(/__tests__/.*|\\.(test))\\.js$",
    "coverageThreshold": {
      "global": {
        "branches": 75,
        "functions": 75,
        "lines": 75,
        "statements": 75
      }
    },
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/fixtures/",
      "/script/"
    ]
  },
  "standard": {
    "parser": "babel-eslint",
    "globals": [
      "jest",
      "it",
      "expect",
      "describe"
    ],
    "ignore": [
      "/src/**/*.test.js"
    ],
    "plugins": [
      "flowtype",
      "react"
    ]
  },
  "peerDependencies": {},
  "dependencies": {},
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-eslint": "^8.0.0",
    "babel-jest": "^20.0.3",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-env": "^1.6.0",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-runtime": "^6.23.0",
    "coveralls": "^2.13.1",
    "cross-env": "^5.0.1",
    "enzyme": "^2.9.1",
    "eslint-plugin-flowtype": "^2.35.1",
    "eslint-plugin-react": "^7.3.0",
    "fetch-mock": "^5.12.2",
    "flow-bin": "^0.54.1",
    "flow-copy-source": "^1.2.0",
    "isomorphic-fetch": "^2.2.1",
    "jest": "^20.0.4",
    "npm-run-all": "^4.0.2",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-test-renderer": "^15.6.1",
    "rimraf": "^2.6.1",
    "snazzy": "^7.0.0",
    "standard": "^10.0.3"
  },
  "scripts": {
    "build": "npm-run-all -l clean babel",
    "babel": "babel src -d . -D -s",
    "flow-copy-source": "flow-copy-source src .",
    "clean": "rimraf index.js index.js.map index.js.flow",
    "flow": "flow --strip-root",
    "prepublish": "npm-run-all -l standard flow test build flow-copy-source",
    "standard": "standard 'src/**/*.js' --verbose --fix | snazzy",
    "test": "cross-env NODE_ENV=test jest",
    "test:coveralls": "cat ./coverage/lcov.info | ./node_modules/.bin/coveralls",
    "test:watch": "npm run test -- --watch"
  }
}
